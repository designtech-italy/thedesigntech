######################
#### DEPENDENCIES ####
######################
<INCLUDE_TYPOSCRIPT: source="FILE:EXT:fluid_styled_content/Configuration/TypoScript/constants.typoscript">


##############################
#### FLUID STYLED CONTENT ####
##############################
styles {
    templates {
        layoutRootPath = EXT:ngs_default_template/Resources/Private/Layouts/ContentElements/
        partialRootPath = EXT:ngs_default_template/Resources/Private/Partials/ContentElements/
        templateRootPath = EXT:ngs_default_template/Resources/Private/Templates/ContentElements/
    }
}


############
### PAGE ###
############
page {
    logo {
        file = EXT:ngs_default_template/Resources/Public/Images/logo.svg
        height = 37
        width = 205
        alt = Designtech
        linktitle = Designtech
    }

    theme {
        navigation {
            style = inverse
        }
    }

    favicon {
        file = EXT:ngs_default_template/Resources/Public/Icons/favicon.ico
    }

    fluidtemplate {
        layoutRootPath = EXT:ngs_default_template/Resources/Private/Layouts/Page/
        partialRootPath = EXT:ngs_default_template/Resources/Private/Partials/Page/
        templateRootPath = EXT:ngs_default_template/Resources/Private/Templates/Page/
    }
    meta {
        description =
        author =
        keywords =
        viewport = width=device-width, initial-scale=1
        robots = index,follow
        apple-mobile-web-app-capable = no
        compatible = IE=edge
    }
    tracking {
        google {
            trackingID =
            anonymizeIp = 1
        }
    }
}


##############
### CONFIG ###
##############
config {
    no_cache = 0
    removeDefaultJS = 0
    admPanel = 1
    prefixLocalAnchors = all
    headerComment = build by sitepackagebuilder.com
    sendCacheHeaders = 1
}
