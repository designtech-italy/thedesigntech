<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<T3DataStructure>
    <sheets>
        <sDEf>
            <ROOT>
                <TCEforms>
                    <sheetTitle>Ngs approve request</sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>

                    <settings.remoteUrl>
                        <TCEforms>
                            <label>Remote Url </label>
                            <config>
                                <type>input</type>
                            </config>
                        </TCEforms>
                    </settings.remoteUrl>

                    <settings.successPage>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>Success page</label>
                            <config>
                                <type>group</type>
                                <internal_type>db</internal_type>
                                <allowed>pages</allowed>
                                <size>1</size>
                                <maxitems>1</maxitems>
                                <minitems>1</minitems>
                                <show_thumbs>1</show_thumbs>
                            </config>
                        </TCEforms>
                    </settings.successPage>

                    <settings.deniedPage>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>Denied page</label>
                            <config>
                                <type>group</type>
                                <internal_type>db</internal_type>
                                <allowed>pages</allowed>
                                <size>1</size>
                                <maxitems>1</maxitems>
                                <minitems>1</minitems>
                                <show_thumbs>1</show_thumbs>
                            </config>
                        </TCEforms>
                    </settings.deniedPage>

                </el>
            </ROOT>
        </sDEf>
    </sheets>

</T3DataStructure>
